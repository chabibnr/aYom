{
    "name": "chabibnr/ayom",
    "description": "Project template for YII2 [AdminLTE3 Yii2 Oauth2 Mimin]",
    "keywords": ["yii2", "framework", "basic","Oauth2","RBAC", "project template"],
    "homepage": "http://www.chabibnr.net/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/chabibnr/ayom/issues?state=open",
        "source": "https://github.com/chabibnr/ayom"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "filsh/yii2-oauth2-server": "2.0.1.x-dev",
        "hscstudio/yii2-mimin": "*",
        "dmstr/yii2-adminlte-asset": "dev-master",
        "kartik-v/yii2-grid": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
